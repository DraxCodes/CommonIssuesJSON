<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Authors>DraxCodes</Authors>
    <Product>CommonIssueJsonParser</Product>
    <Description>A Json Parser for the common-issues repo.</Description>
    <PackageLicenseUrl>https://github.com/DraxCodes/CommonIssuesJSON/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>Copyright (c) 2019 Joel Parkinson</Copyright>
    <PackageProjectUrl>https://github.com/DraxCodes/CommonIssuesJSON</PackageProjectUrl>
    <RepositoryUrl>https://github.com/DraxCodes/CommonIssuesJSON</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
